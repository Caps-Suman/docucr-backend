    "billing_guidelines": [
        {
            "id": "bg_ai_0",
            "title": "Infusion Claim Billing",
            "description": "For Infusion claim if buy and bill is mentioned then to bill with amount, if special pharmacy is mentioned then bill with zero amount and for Medicare special pharmacy case take $0.01 as billed amount."
        },
        {
            "id": "bg_ai_1",
            "title": "Modifier Usage for Insurances",
            "description": "For insurances Molina, Medicaid & Aetna Better Health use only LT & RT modifier according to which side marked on superbill, do not use modifier 50 with X-ray codes."
        },
        {
            "id": "bg_ai_2",
            "title": "CPT Code Replacements",
            "description": "M54.50 replace with M54.59"
        },
        {
            "id": "bg_ai_3",
            "title": "CPT Code Replacements",
            "description": "M25.50 replace with M25.59"
        },
        {
            "id": "bg_ai_4",
            "title": "CPT Code Replacements",
            "description": "M06.06 replace with M06.069"
        },
        {
            "id": "bg_ai_5",
            "title": "Bilateral ICD Usage",
            "description": "Use M17.0 Bilateral ICD with X-ray code (only when M17.12, M17.9 &M17.11 is given) if bilateral. Other use only pain in Knee ICD code according to which side X-ray done."
        },
        {
            "id": "bg_ai_6",
            "title": "Bilateral ICD Usage",
            "description": "Use M16.0 Bilateral ICD with X-ray code (only when M16.12, M16.9 &M16.11 is given) if bilateral. Other use only pain in Hip ICD code according to which side X-ray done."
        },
        {
            "id": "bg_ai_7",
            "title": "ICD Code Restrictions",
            "description": "If M15.0 is used don’t take ICD code from range (M16 to M19)."
        },
        {
            "id": "bg_ai_8",
            "title": "ICD Code Restrictions",
            "description": "If M13.0 is used don’t take ICD from range (M15 to M19)."
        },
        {
            "id": "bg_ai_9",
            "title": "Modifier Usage for J Codes",
            "description": "Please use JZ modifier in all J code CPTs for all the insurance."
        },
        {
            "id": "bg_ai_10",
            "title": "Modifier Usage for Medicare",
            "description": "JA modifier should be only use with Medicare for CPT J0129."
        },
        {
            "id": "bg_ai_11",
            "title": "Modifier Usage for CPT Codes",
            "description": "LT or RT modifier use in Medicare with CPT 73130 and 73110 and don’t use 50 modifiers for these CPT code. LT and RT bill with 2 unit."
        },
        {
            "id": "bg_ai_12",
            "title": "JW Modifier Usage",
            "description": "If wastage as marked on super bill given then take only JW modifier with drug codes do not use JZ & JW modifier both on drug codes."
        },
        {
            "id": "bg_ai_13",
            "title": "Code Restrictions",
            "description": "Do not bill both codes together M45.9 and M05.89."
        },
        {
            "id": "bg_ai_14",
            "title": "Code Restrictions",
            "description": "Do not bill both codes together M13.0 and M15.1."
        },
        {
            "id": "bg_ai_15",
            "title": "Primary DX Code Restrictions",
            "description": "Do Not Bill M1610 DX code as primary."
        },
        {
            "id": "bg_ai_16",
            "title": "Code Restrictions",
            "description": "Do not Bill M19.90 & M13.0 DX code together."
        },
        {
            "id": "bg_ai_17",
            "title": "Code Restrictions",
            "description": "Do not Bill M06.4 & M13.0 DX code together."
        },
        {
            "id": "bg_ai_18",
            "title": "X-ray Code Usage",
            "description": "M17.11 and M17.12 use only when LT RT given with x-ray codes. If marked on superbill."
        },
        {
            "id": "bg_ai_19",
            "title": "DX Code Usage for CPT J2507",
            "description": "M1A9XX1 DX code use for CPT J2507."
        },
        {
            "id": "bg_ai_20",
            "title": "ICD Code Usage",
            "description": "M06.09 is given then take M06.9."
        },
        {
            "id": "bg_ai_21",
            "title": "ICD Code for CMC Joint",
            "description": "B/L first CMC joint ICD 10- M18.0."
        },
        {
            "id": "bg_ai_22",
            "title": "CPT Code Usage",
            "description": "CPT 73502 marked on super bill with LR & RT modifier both then use 73521 CPT code. If only LT or RT given then use 73502 with modifier as per given side on superbill."
        },
        {
            "id": "bg_ai_23",
            "title": "Modifier Restrictions for J Codes",
            "description": "Don’t use JZ modifier with CPT J1200 & J2919 Only for Anthem Health insurance."
        },
        {
            "id": "bg_ai_24",
            "title": "Admin Code Usage for CPT J0897",
            "description": "For CPT J0897 use admin code 96372 with Aetna Better Health, Medicare & UHC. For rest insurance use admin code 96401."
        },
        {
            "id": "bg_ai_25",
            "title": "CPT Code N95.8 and M85.88 Usage",
            "description": "CPT code 77080 marked on superbill need to take N95.8 for Female and M85.88 for Male."
        },
        {
            "id": "bg_ai_26",
            "title": "Telehealth Visit Billing",
            "description": "For Telehealth Visit use POS 10 and CPT code according to super bill and time for commercial."
        },
        {
            "id": "bg_ai_27",
            "title": "Medicare Telehealth Billing",
            "description": "For Medicare do not bill telehealth CPT code, use Modifier 95 and E&M."
        },
        {
            "id": "bg_ai_28",
            "title": "CPT J1010 Billing Limit",
            "description": "CPT J1010 maximum can be billed with 120 units only."
        },
        {
            "id": "bg_ai_29",
            "title": "Unspecified Code Usage",
            "description": "In Aetna Better & Sentara try not to use unspecified code as primary DX."
        },
        {
            "id": "bg_ai_30",
            "title": "Aetna Policy Bulletin 0810",
            "description": "As per Aetna Policy Bulletin 0810 administration code 96413 is no valid a valid code to be billed along with CPT J2507 (Krystexxa) also verified with the coding team, CPT 96365 must be billed as an admin code & 96366 for each additional hour."
        },
        {
            "id": "bg_ai_31",
            "title": "CPT Code 73502 & 72170 Billing",
            "description": "73502 & 72170 can be billed together use 59 modifier with 72170 and different ICD."
        },
        {
            "id": "bg_ai_32",
            "title": "CPT Code 73521 & 72170 Billing",
            "description": "73521 & 72170 can be billed together use 59 modifier with 72170 and different ICD."
        },
        {
